package entity;

public class User {
	private String id;
	private String name;
	private String phoneNumber;
	private String pinCode;

	private static int uId;
	static {
		uId = 100;
	}

	public User() {

	}

	public User(String name, String phoneNumber, String pinCode) {
		this.id = "u" + ++uId;
		this.name = name;
		this.phoneNumber = phoneNumber;
		this.pinCode = pinCode;
	}

}
