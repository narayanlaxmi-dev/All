package day3_1practice;

public class operatorMCQ {
	public static void main(String[] args) {
		int x = 10, y = 15, z = 2;
		if (++x < 10 || ++y > 15) {
			x++;
		} else {
			y++;
		}
		System.out.println(x + "---" + y);
		if (++x < 10 && ++y > 15) {
			x++;
		} else {
			y++;
		}
		System.out.println(x + "---" + y);
		System.out.println(++z + --z + z++ - ++z);
	}
}
