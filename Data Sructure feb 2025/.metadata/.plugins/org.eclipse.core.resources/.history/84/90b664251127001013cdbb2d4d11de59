package com.dsa.mam;

import java.util.Scanner;

public class FibonacciSeries {
	public static void main(String[] args) {
		int num = 0;
		System.out.println("---Factorial Program---\n negative number:-0\n\n Enter number: ");
		Scanner sc = new Scanner(System.in);
		num = sc.nextInt();
		for(int i=0; i<num; i++)
			System.out.println("The number sent" + num + " factorial is : " + fibonaccilfun(i));
	}

	public static int fibonaccilfun(int x) {
		if (x <= 1)
			return x;
		return fibonaccilfun(x - 1) + fibonaccilfun(x - 2);

	}
}
